PORT=8001
HOST=0.0.0.0
NODE_ENV=production

WA_API_KEY=change-me
WA_START_ON_BOOT=true
WA_SESSION_CLIENT_ID=main
WA_AUTH_DATA_PATH=./.wwebjs_auth
WA_MAX_MESSAGE_LENGTH=4096

WA_RECONNECT_ENABLED=true
WA_RECONNECT_INITIAL_DELAY_MS=5000
WA_RECONNECT_MAX_DELAY_MS=60000

LOG_LEVEL=info
REQUEST_BODY_LIMIT=256kb

# Local dev (optional). In Docker/Render the service Dockerfile installs Chromium.
# PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium
WA_PUPPETEER_HEADLESS=true

